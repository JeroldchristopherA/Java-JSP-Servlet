<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>Archetype Created Web Application</display-name>


	<servlet>

		<servlet-name>a</servlet-name>
		<servlet-class>com.edu.basic.Static</servlet-class>

	</servlet>



	<servlet>
		<servlet-name>b</servlet-name>
		<servlet-class>com.edu.basic.Dynamic</servlet-class>

	</servlet>




	<servlet>
		<servlet-name>c</servlet-name>
		<servlet-class>com.edu.login.loginSer</servlet-class>
	</servlet>
	
	
	
	<servlet>
		<servlet-name>d</servlet-name>
		<servlet-class>com.edu.login.DemoStudentlogin</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>a</servlet-name>
		<url-pattern>/Static</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>b</servlet-name>
		<url-pattern>/Dynamic</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>c</servlet-name>
		<url-pattern>/loginSer</url-pattern>
	</servlet-mapping>
	
	
	<servlet-mapping>
	<servlet-name>d</servlet-name>
	<url-pattern>/DemoStudentlogin</url-pattern>
	
	
	</servlet-mapping>

</web-app>

